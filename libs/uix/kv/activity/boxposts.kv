#: kivy 1.9.1

# View для вывода списка постов группы.

<BoxPosts>:
    name: 'box posts'

    BoxLayout:
        id: box_paginator
        orientation: 'vertical'
        spacing: dp(5)

        ScrollView:

            GridLayout:
                id: box_posts
                cols: 1
                size_hint_y: None
                height: self.minimum_height
